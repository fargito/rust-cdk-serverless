{
  "name": "rust-workspaces",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "bootstrap": "cdk bootstrap",
    "deploy": "cdk deploy",
    "destroy": "cdk destroy",
    "lint-fix": "pnpm linter-base-config --fix",
    "lint-fix-all": "pnpm lint-fix .",
    "linter-base-config": "eslint --ext=js,ts",
    "test": "pnpm test-linter && pnpm test-type && pnpm test-unit && pnpm test-cdk",
    "test-cdk": "cdk synth --quiet",
    "test-linter": "pnpm linter-base-config .",
    "test-type": "tsc",
    "test-unit": "vitest run --coverage",
    "test-integration": "vitest run --config vite.config-integration.ts",
    "watch": "tsc -w"
  },
  "devDependencies": {
    "@aws-crypto/sha256-js": "^5.2.0",
    "@aws-sdk/client-cloudformation": "^3.552.0",
    "@aws-sdk/credential-providers": "^3.552.0",
    "@aws-sdk/types": "^3.535.0",
    "@event-scout/client": "^0.7.0",
    "@event-scout/construct": "^0.7.0",
    "@smithy/signature-v4": "^2.3.0",
    "@swc-node/register": "^1.9.0",
    "@types/node": "^20.9.1",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "@vitest/coverage-v8": "^1.4.0",
    "aws-cdk": "2.140.0",
    "aws-cdk-lib": "2.140.0",
    "axios": "^1.6.8",
    "constructs": "^10.0.98",
    "dotenv": "^16.4.5",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.0",
    "syncpack": "^12.3.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-tsconfig-paths": "^4.0.5",
    "vitest": "^1.4.0"
  },
  "engines": {
    "node": "^20.0.0"
  },
  "packageManager": "pnpm@9.1.0"
}
